package com.Class12;

public class Recap {

	public static void main(String[] args) {
		
		int [][] arr= {
				{1,2,3,},//first array that stored with 0
				{10,20,30},//second array with index 1
				{100,200,300,}//3 array with index 2
		};
		//how many arrays in total
		for (int i=0; i<arr.length; i++) {
			for (int j=0; j<arr[i].length; j++) { //0 //1 //2
				System.out.print(arr[i][j]+" ");
				
			}
			
			System.out.println();
			
		}

	}

}
