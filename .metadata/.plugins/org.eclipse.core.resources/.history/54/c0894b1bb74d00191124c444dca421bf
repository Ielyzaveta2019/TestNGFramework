//Create an array of countries: north america countries, south america countries,
//europe countries, asian countries,african countries.Then print all values from that array.
	
	String [][] countries= {{"Canada", "USA", "Mexico"},
			                {"Brazil", "Columbia", "Argentina","Chile"},
	                        {"Ukraine","Portugal","Spain","Italy","Poland"},
	                        {"Turkey","China","Japan","India"},
	                        {"Egypt", "Marocco","Mauritania","Nigeria"}
	};
	for (int i=0; i<countries.length; i++) {
		for (int j=0; j<countries[i].length; j++) {
			System.out.println(countries [i][j]+ " ");
				
			}
		System.out.println();
		}
	}
}





































































































































































































































