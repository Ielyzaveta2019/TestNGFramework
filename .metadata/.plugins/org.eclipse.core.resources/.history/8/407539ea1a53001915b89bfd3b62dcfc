        int [] num = {9,4,3,8,12};
        int largest = num[0], temp =0;
        for (int i=0; i< num.length-1;i++) {
        	for(int j=0; j<num.length-1; j++) {
        		if(num[i] > num[i+1]) {
        			temp = num[i];
        			num[i] = num[i+1];
        			num[i+1] = temp;
        		}
        	}
        }
        for(int x=0; x<num.length-1; x++) {
        	System.out.print(num[x]+", ");
        }
        
