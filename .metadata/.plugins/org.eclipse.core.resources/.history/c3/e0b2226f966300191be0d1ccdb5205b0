package com.class24;

public class SuperKeyword {
	String name ="John";
	
	public void sayName() {
		System.out.println("My name is "+name);
	}
}
class ChildOfSuperKeyword extends SuperKeyword{
	String name="Michael";
	
	public void syaName() {
		System.out.println("Parent name is "+super.name);
		System.out.println("Child name is "+name);
	}
	
}
