package com.class16_2;

public class Task2 {

	public static void main(String[] args) {
		

		
		/*the number of my columns is always less or equal to the number of my rows
		 * Print the following pattern:
		 * 
		 * 1 
		 * 12 
		 * 123 
		 * 1234
		 * 12345
		 * 
		 */
		
		
		for(int i =1; i<=5;i++) {
			
			
			for(int y =1; y<=i; y++) {
				System.out.print(y);
				
			}
			
			System.out.println();
		}
		
		
		
		
		
		
	}

}
